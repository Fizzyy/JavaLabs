<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">
    <context-param>
        <param-name>URL1</param-name>
        <param-value>http://localhost:8080/AS_ydssi/secondPage.html</param-value>
    </context-param>
    <context-param>
        <param-name>URL2</param-name>
        <param-value>http://localhost:8080/AS_ydssi/secondServlet</param-value>
    </context-param>

    <filter>
        <filter-name>F1</filter-name>
        <filter-class>lb19.F1</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F1</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>F2</filter-name>
        <filter-class>lb19.F2</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F2</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>F2</filter-name>
        <url-pattern>/Ccc.jsp</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>F3</filter-name>
        <filter-class>lb19.F3</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F3</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>
</web-app>
